#include <stdio.h>
#include<string.h>
int main()
{
    int length,temp=0;
    char a[10];
   scanf("%s%s",a);
   length=strlen(a);
  
     for (int i=0, j =length-1;i<length;i++,j--)
     {
     
        if (a[i]==a[j])
        {
            temp++;
        }
        
     }
     if (temp==length)
     {
        printf("This string is palindrome %d",temp); 
     }
     else 
     {
          printf("This string is not palindrome %d",temp);  
     }
     
  return 0;
}
